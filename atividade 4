def is_prime(num):
    if num <= 1: return ("neutro")
    for i in range(2, int(num**0.5) + 1):
        if num % i == 0: return ("par")
    return ("primo")
print("Status de 'primo' para nÃºmeros de 1 a 100:")
for num in range(1, 101):
    status = is_prime(num)
    print(f"NÃºmero {num}: {status}")
    
